<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>PosterServlet</servlet-name>
        <servlet-class>Poster</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>BatcherServler</servlet-name>
        <servlet-class>Batcher</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>LikerServlet</servlet-name>
        <servlet-class>Liker</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PosterServlet</servlet-name>
        <url-pattern>/post</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>BatcherServler</servlet-name>
        <url-pattern>/batch</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LikerServlet</servlet-name>
        <url-pattern>/like</url-pattern>
    </servlet-mapping>

</web-app>
